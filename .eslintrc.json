{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "import/extensions": "off",
        "indent": "off",
        "no-console": [
            "error", 
             { 
               "allow": ["warn", "error","info"]
             }
        ],
        "no-use-before-define": 
        ["error",
         {
            "functions": false,
            "classes": true,
            "variables": true,
            "allowNamedExports": false
        }]
        ,
        "no-await-in-loop": "off",
        "class-methods-use-this": "off",
        "no-unused-vars": "warn"
    }
}
